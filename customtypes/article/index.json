{
  "id": "article",
  "label": "Article",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "uid": { "type": "UID", "config": { "label": "Uid", "placeholder": "" } },
      "title": {
        "type": "Text",
        "config": { "label": "Title", "placeholder": "" }
      },
      "subtitle": {
        "type": "StructuredText",
        "config": {
          "label": "Subtitle",
          "placeholder": "",
          "allowTargetBlank": true,
          "single": "paragraph"
        }
      },
      "publish_date": {
        "type": "Date",
        "config": { "label": "Publish Date", "placeholder": "" }
      },
      "slices": {
        "type": "Slices",
        "fieldset": "Slice Zone",
        "config": {
          "choices": {
            "text": { "type": "SharedSlice" },
            "quote": { "type": "SharedSlice" },
            "image": { "type": "SharedSlice" },
            "code": { "type": "SharedSlice" }
          }
        }
      }
    }
  }
}
